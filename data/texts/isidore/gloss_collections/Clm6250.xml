         <div><!--1.I.2__5-->
            <note target="#b1.I.2__5" n="L1.2.5_2" hand="hand-g1">
               <gloss>a similitudine</gloss>
            </note>
            <!--1.III.1__3-->
            <note target="#b1.III.1__3" n="L3.1.3_4" hand="hand-g2">
               <gloss>scriptores</gloss>
            </note>
            <!--1.III.5__2-->
            <note target="#b1.III.5__2" n="L3.5.2_4">
               <gloss>dicimus</gloss>
            </note>
            <!--1.III.5__14-->
            <note target="#b1.III.5__14" n="L3.5.14_4" hand="hand-g4">
               <gloss>Ieras id est
               sacerdotales, nam ierontas Graeci valde senes dicunt; pandemos vero vulgares, nam pan
               dicitur omne</gloss>
            </note>
            <!--1.III.5__16-->
            <note target="#b1.III.5__16" n="L3.5.16_7">
               <gloss>Phoenices dicti a rubro mare, quod proficit in
                  Syriam, qui Sidonem urbem inibi condiderunt</gloss>
               <note type="editorial">Integrated into the main
                  text</note>
            </note>
            <!--1.III.7__11-->
            <note target="#b1.III.7__11" n="L3.7.11_1" hand="hand-g4">
               <gloss>versus
                  her<unclear>oicus</unclear>
               </gloss>
            </note>
            <!--1.IV.12__2-->
            <note target="#b1.IV.12__2" n="L4.12.2_1" hand="hand-g4">
               <gloss>id est gymnasii</gloss>
            </note>
            <!--1.VI.2__12-->
            <note target="#b1.VI.2__12" n="L6.2.12_3">
               <gloss>orationis</gloss>
            </note>
            <!--1.VII.2__3-->
            <note target="#b1.VII.2__3" n="L7.2.3_4">
               <gloss>vocati sunt</gloss>
            </note>
            <!--1.IX.3__4-->
            <note target="#b1.IX.3__4" n="L9.3.4_3">
               <gloss>Nam perfecta a
               perficiendo ut lego. Tunc enim dicendum est quando in praesenti tempore
               legimus</gloss>
            </note>
            <!--1.XVIII.4__1-->
            <note target="#b1.XVIII.4__1" n="L18.4.1_1">
               <gloss>habebit</gloss>
            </note>
            <!--1.XXX.1__6-->
